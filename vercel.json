{
    "rewrites": [
        {
            "source": "/api/iptv",
            "destination": "/api/iptv.js"
        },
        {
            "source": "/api/scores/(.*)",
            "destination": "/api/scores.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}